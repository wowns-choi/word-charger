<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="firstportfolio.wordcharger.repository.WordMapper">

    <select id="findByWordId" resultType="String">
        SELECT word FROM word WHERE WORD_ID = #{wordId}
    </select>

    <select id="findByWord" resultType="Integer">
        SELECT word_id FROM word WHERE WORD = #{word}
    </select>



</mapper>
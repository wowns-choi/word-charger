<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="firstportfolio.wordcharger.repository.WrongMeaningMapper">


    <select id="findWrongMeaning" resultType="String">
        SELECT wrong_meaning FROM wrong_meaning
        WHERE word_id = #{wordId}
    </select>



</mapper>
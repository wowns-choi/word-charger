<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="firstportfolio.wordcharger.repository.MeaningMapper">


    <select id="findMeaning" resultType="String">
        SELECT word_meaning FROM meaning
        WHERE word_id = #{wordId}
    </select>



</mapper>